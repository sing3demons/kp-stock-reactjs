rom '@material-ui/core'\nimport { makeStyles } from '@material-ui/core/styles'\nimport logo_auth from 'assets/images/authen_header.jpg'\nimport { useHistory } from 'react-router-dom'\nimport * as loginAcction from 'modules/actions/login.action'\nimport { useDispatch, useSelector } from 'react-redux'\nimport { Alert, AlertTitle } from '@material-ui/lab'\n\nconst useStyles = makeStyles((theme) => ({\n  root: {\n    alignContent: 'center',\n    alignItems: 'center',\n    maxWidth: 345,\n  },\n  media: {\n    height: 200,\n  },\n  submit: {\n    margin: theme.spacing(3, 0, 2),\n  },\n  content: {\n    // display: 'flex',\n    padding: theme.spacing(2),\n  },\n}))\n\nexport default function Login() {\n  const classes = useStyles()\n  const history = useHistory()\n  const dispatch = useDispatch()\n  const loginReducer = useSelector((state) => state.loginReducer)\n\n  const [account, setAccount] = useState({\n    username: 'admin',\n    password: '1234',\n  })\n\n  const goToRegister = () => history.push('/users/register')\n\n  return (\n    <Container style={{ display: 'flex', justifyContent: 'center' }}>\n      <Card className={classes.root}>\n        <CardMedia\n          className={classes.media}\n          image={logo_auth}\n          title=\"Contemplative Reptile\"\n        />\n        <CardContent>\n          <Typography gutterBottom variant=\"h5\" component=\"h2\">\n            Login\n          </Typography>\n          <form\n            className={classes.form}\n            noValidate\n            onSubmit={(e) => {\n              e.preventDefault()\n              dispatch(loginAcction.login({ ...account, history }))\n            }}\n          >\n            <TextField\n              variant=\"outlined\"\n              margin=\"normal\"\n              required\n              fullWidth\n              value={account.username}\n              onChange={(e) => {\n                setAccount({ ...account, username: e.target.value })\n              }}\n              name=