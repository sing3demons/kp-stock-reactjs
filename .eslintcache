rt { Alert, AlertTitle } from '@material-ui/lab'\n\nconst useStyles = makeStyles((theme) => ({\n  root: {\n    alignContent: 'center',\n    alignItems: 'center',\n    maxWidth: 345,\n  },\n  media: {\n    height: 200,\n  },\n  submit: {\n    margin: theme.spacing(3, 0, 2),\n  },\n  content: {\n    // display: 'flex',\n    padding: theme.spacing(2),\n  },\n}))\n\nexport default function Login() {\n  const classes = useStyles()\n  const history = useHistory()\n  const dispatch = useDispatch()\n  const loginReducer = useSelector((state) => state.loginReducer)\n\n  const [account, setAccount] = useState({\n    username: 'admin',\n    password: '1234',\n  })\n\n  const goToRegister = () => history.push('/users/register')\n\n  return (\n    <Container style={{ display: 'flex', justifyContent: 'center' }}>\n      <Card className={classes.root}>\n        <CardMedia\n          className={classes.media}\n          image={logo_auth}\n          title=\"Contemplative Reptile\"\n        />\n        <CardContent>\n          <Typography gutterBottom variant=\"h5\" component=\"h2\">\n            Login\n          </Typography>\n          <form\n            className={classes.form}\n            noValidate\n            onSubmit={(e) => {\n              e.preventDefault()\n              dispatch(loginAcction.login({ ...account, history }))\n            }}\n          >\n            <TextField\n              variant=\"outlined\"\n              margin=\"normal\"\n              required\n              fullWidth\n              value={account.username}\n              onChange={(e) => {\n                setAccount({ ...account, username: e.target.value })\n              }}\n              name=\"username\"\n              id=\"username\"\n              label=\"Username\"\n              autoComplete=\"email\"\n              autoFocus\n            />\n            <TextField\n              variant=\"outlined\"\n              margin=\"normal\"\n              required\n              fullWidth\n              value={account.password}\n              onChange={(e) => {\n                setAccount({ ...account, password: e.target.value })\n              }}\n              name=\"password\"\n              label=\"password\"\n              id=\"password\"\n              type=\"password\"\n              autoComplete=\"current-password\"\n            />\n\n            {loginReducer.error && (\n              <Alert severity=\"error\">\n                <AlertTitle>Error</AlertTitle>\n                {loginReducer.result}\n              </Alert>\n            )}\n\n            <Button\n              type=\"submit\"\n              fullWidth\n              variant=\"contained\"\n              color=\"primary\"\n              className={classes.submit}\n            >\n              Sign In\n            </Button>\n            <Button\n              fullWidth\n              size=\"small\"\n              color=\"primary\"\n              onClick={goToRegister}\n            >\n              Register\n            </Button>\n          </form>\n        </CardContent>\n      </Card>\n    </Container>\n  )\n}\n","/Users/sing3demons/Dev_Code/React/kp-stock-reactjs/src/modules/ui/components/Routes.js",[],"/Users/sing3demons/Dev_Code/React/kp-stock-reactjs/src/modules/users/components/Register.js",[],"/Users/sing3demons/Dev_Code/React/kp-stock-reactjs/src/modules/stock/components/Stock.js",["113","114","115"],"import React, { useEffect } from 'react'\nimport { useDispatch, useSelector } from 'react-redux'\nimport { Link } from 'react-router-dom'\nimport NumberFormat from 'react-number-format'\nimport MaterialTable, { MTableToolbar } from 'material-table'\nimport { makeStyles } from '@material-ui/core/styles'\nimport { Button, Typography } from '@material-ui/core'\nimport { imageUrl } from './../../Constants'\nimport Moment from 'react-moment'\nimport * as TableIcon from './TableIcons'\nimport * as stockActions from 'modules/actions/stock.action'\n\nconst tableIcons = TableIcon\n\nexport default function Stock() {\n  const dispatch = useDispatch()\n  const stockReducer = useSelector(({ stockReducer }) => stockReducer)\n\n  useEffect(() => {\n    dispatch(stockActions.getProducts())\n  }, [])\n\n  const columns = [\n    {\n      title: 'Id',\n      render: (item) => <Typography variant=\"body1\">{item.id}</Typography>,\n    },\n    {\n      title: 'Image',\n      cellStyle: { padding: 0 },\n      render: (item) => (\n        <img\n          src={`${imageUrl}/images/${item.image}?dummy=${Math.random()}`}\n          style={{ width: 70, height: 70, borderRadius: '5%' }}\n        />\n      ),\n    },\n    {\n      title: 'Name',\n      cellStyle: { minWidth: 700 },\n      render: (item) => <Typography variant=\"body1\">{item.name}</Typography>,\n    },\n\n    {\n      title: 'Price',\n      render: (item) => (\n        <Typography variant=\"body1\">\n          <NumberFormat\n            value={item.price}\n            displayType={'text'}\n            thousandSeparator={true}\n            decimalScale={2}\n            fixedDecimalScale={true}\n            prefix={'฿'}\n          />\n        </Typography>\n      ),\n    },\n    {\n      title: 'Stock',\n      render: (item) => (\n        <Typography variant=\"body1\">\n          <NumberFormat\n            value={item.stock}\n            displayType={'text'}\n            thousandSeparator={true}\n            decimalScale={0}\n            fixedDecimalScale={true}\n            suffix={' pcs'}\n          />\n        </Typography>\n      ),\n    },\n    {\n      title: 'Updated',\n      render: (item) => (\n        <Typography>\n          <Moment format=\"DD/MM/YYYY\">{item.updatedAt}</Moment>\n        </Typography>\n      ),\n    },\n  ]\n\n  return (\n    <>\n      <MaterialTable\n        icons={tableIcons.TableIcon}\n        columns={columns}\n        data={stockReducer.result ? stockReducer.result : []}\n        title=\"Stock\"\n        components={{\n          Toolbar: (props) => (\n            <div>\n      